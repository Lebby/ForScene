<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="forscene"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <fileSets>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>html/src</directory>
      <includes>
        <include>**/*.html</include>
        <include>**/*.java</include>
        <include>**/*.xml</include>
        <include>**/*.properties</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>flash</directory>
      <includes>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>android</directory>
      <includes>
        <include>**/*.xml</include>
        <include>**/*.properties</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>ios</directory>
      <includes>
        <include>**/*.xml</include>
        <include>**/*.cs</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>android/target</directory>
      <includes>
        <include>**/*.java</include>
        <include>**/*.properties</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>html</directory>
      <includes>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>android/src</directory>
      <includes>
        <include>**/*.java</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory>android/res</directory>
      <includes>
        <include>**/*.xml</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>html/src</directory>
      <includes>
        <include>**/*.ico</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>android</directory>
      <includes>
        <include>**/*.cfg</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>ios</directory>
      <includes>
        <include>**/*.png</include>
        <include>**/*.sln</include>
        <include>**/*.plist</include>
        <include>**/*.csproj</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>android/target</directory>
      <includes>
        <include>**/*.ap_</include>
        <include>**/*.so</include>
        <include>**/*.jar</include>
        <include>**/*.dex</include>
        <include>**/*.class</include>
        <include>**/*.apk</include>
      </includes>
    </fileSet>
    <fileSet encoding="UTF-8">
      <directory>android/res</directory>
      <includes>
        <include>**/*.png</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" encoding="UTF-8">
      <directory></directory>
      <includes>
        <include>build.xml</include>
      </includes>
    </fileSet>
  </fileSets>
  <modules>
    <module id="${rootArtifactId}-core" dir="core" name="${rootArtifactId}-core">
      <fileSets>
        <fileSet filtered="true" packaged="true" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.java</include>
          </includes>
        </fileSet>
        <fileSet packaged="true" encoding="UTF-8">
          <directory>src/main/java</directory>
          <includes>
            <include>**/*.png</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
  </modules>
</archetype-descriptor>
